{
  "name": "axiamatic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client:install": "npm install --prefix client",
    "server:install": "npm install --prefix server",
    "install:run": "npm run client:install && npm run server:install && npm install && npm run dev",
    "server": "nodemon ./server/server.js",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server:start": "node ./server/server.js",
    "client:start": "npm run dev --prefix client",
    "start": "concurrently \"npm run server:start\" \"npm run client:start\""
  },
  "keywords": [],
